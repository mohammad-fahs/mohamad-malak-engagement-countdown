{
    "version": 2,
    "builds": [
        {
            "src": "client/vite.config.ts",
            "use": "@vercel/static-build"
        },
        {
            "src": "server/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/public/$1"
        }
    ]
}